store custom items:
// compute fish token value
    const avgFishTokenPerUnit = (() => {
      const { totalRatio, count } = Object.entries({
        "tentacled temptation": 225,
        "inflated delicacy": 563,
        "prismatic delight": 749,
      }).reduce(
        (acc, [specialName, tokenValue]) => {
          const item = normalizedItems.find((i) => i.name === specialName);
          if (item?.value) {
            acc.totalRatio += item.value / tokenValue;
            acc.count += 1;
          }
          return acc;
        },
        { totalRatio: 0, count: 0 },
      );
      let avg = count > 0 ? totalRatio / count : 1;
      return avg > 0 ? avg : 1;
    })();

    normalizedItems.push({
      name: "fish tokens",
      value: avgFishTokenPerUnit,
    });

    // Add Fishing Items
    Object.entries({
      harpoon: 38,
      "fishing bow": 64,
      net: 49,
      "fishing rod": 11,
      "deadly bait": 282,
      "golden bait": 27,
      "lucky bait": 90,
      "xp bait": 113,
      "timely bait": 45,
      // NOT THERE IN SHOP (PERSONAL OPINION)
      dynamite: 150,
      "magnet fishing rope": 200,
      "eyeball bait": 400,
      "farmer bait": 150,
      "ghastly bait": 100,
      "gift bait": 500,
      "jerky bait": 500,
    }).forEach(([name, tokenCount]) => {
      normalizedItems.push({
        name,
        value: tokenCount * avgFishTokenPerUnit,
      });
    });

    // for all of these inserts do fishing: true

